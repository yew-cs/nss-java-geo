VIEW NSS_GEOMETRYVIEW AS
SELECT
    ID,
    CREATEDAT,
    CREATEDBY,
    MODIFIEDAT,
    MODIFIEDBY,
    VALIDFROM,
    VALIDTO,
    BOGEOMETRY.ST_AsGeoJSON() AS BOGEOMETRY,
    BOGEOMETRY.ST_GeometryType() AS GEOMETRYTYPE,
    BOREFERENCE_ID,
    BOCONTEXT,
    METADATA,
    ISMARKEDFORDELETION
FROM
"NSS_GEOMETRIES" 
