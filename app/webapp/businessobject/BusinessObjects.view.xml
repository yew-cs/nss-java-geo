<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:semantic="sap.m.semantic" height="100%" controllerName="com.sap.nss.nsaa.businessobject.BusinessObjects" displayBlock="true">
	<SplitContainer>
		<masterPages>
			<Page id="master" title="Business Objects" icon="sap-icon://action" class="sapUiStdPage">
				<customHeader>
					<Bar>
						<contentLeft>
							<Text text="BusinessObjects"/>
						</contentLeft>
						<contentRight>
							<Button id="add" text="Add" press="handleEditPress"/>
						</contentRight>
					</Bar>
				</customHeader>
				<content>
					<List mode="Delete" items="{/BusinessObjectTypes}">
						<items>
							<StandardListItem title="{Name}" type="Inactive" description="{Description}"/>
						</items>
					</List>
				</content>
			</Page>
		</masterPages>
		<detailPages>
			<Page id="page" showHeader="true">
				<customHeader>
					<Bar>
						<contentRight>
							<Button id="edit" text="Edit" press="handleEditPress"/>
							<Button id="save" text="Save" type="Emphasized" visible="false" press="handleSavePress"/>
							<Button id="cancel" text="Cancel" visible="false" press="handleCancelPress"/>
						</contentRight>
					</Bar>
				</customHeader>
			</Page>
		</detailPages>
	</SplitContainer>
</mvc:View>