<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormChange471" editable="true">
			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1"
					emptySpanM="1" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="Action Name">
							<f:fields>
								<Input value="{ActionName}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Description">
							<f:fields>
								<Input value="{Description}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Applies to">
							<f:fields>
								<MultiInput id="multiInput" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested"></MultiInput>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Link to API">
							<f:fields>
								<Input value="{LinkToApi}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Interaction">
							<f:fields>
								<Select forceSelection="false" selectedKey="{/Interaction}">
									<core:Item key="Single Click / Tap" text="Single Click / Tap"/>
									<core:Item key="Double Click / Tap" text="Double Click / Tap"/>
								</Select>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>