<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormDisplay471" editable="false">
			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1"
					emptySpanM="1" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="Action Name">
							<f:fields>
								<Text text="{ActionName}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Description">
							<f:fields>
								<Text text="{Description}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Applies to">
							<f:fields>
								<Text text="{AppliesTo}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Link to API">
							<f:fields>
								<Text text="{LinkToApi}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Interaction">
							<f:fields>
								<Text text="{Interaction}"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>